.el-main
    padding 10px 80px

.table_content
    width 100%
    //box-shadow: 0 2px 12px 0 rgba(0, 0, 0, .1);
    border-radius 4px
    margin-top 10px

.el-table
    border-radius 4px 4px 0 0

.el-table td
    border-bottom 1px solid #eee
    padding 5px 0
    font-size 13px
    cursor pointer

.el-table tbody tr:nth-child(odd)
    background-color #f5f5f5

.el-dialog
    .el-dialog__header
        border-bottom 1px solid #dbdbdb

    .el-form

        .el-form-item
            margin-right 80px
