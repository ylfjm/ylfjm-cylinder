.el-main
    padding 0
    margin 0

.el-table
    border-radius 4px 4px 0 0

.table-content
    width 100%
    border-radius 4px
    margin-top 10px

.el-table td
    border-bottom 1px solid #eee
    padding 5px 0
    font-size 13px
    cursor pointer

.el-table tbody tr:nth-child(odd)
    background-color #f5f5f5

.el-dialog
    .el-dialog__header
        border-bottom 1px solid #dbdbdb

    .el-form

        .el-form-item
            margin-right 80px
